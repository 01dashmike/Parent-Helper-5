// Note: This config supports tooling (e.g., Next.js config transpilation).
// It is not directly used by the Next.js app build.
{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "next.config.mjs"
  ]
}
